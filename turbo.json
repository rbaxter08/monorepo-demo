{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["test", "^build"],
      "outputs": ["out/**", "dist/**"]
    },
    "frontend-app#build": {
      "dependsOn": ["backend#build", "test"],
      "outputs": ["out/**"]
    }
  }
}
